<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent" xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_gravity="center"
             tools:context=".ui.daily.fragments.TimerFragment">
    
    <include
            android:id="@+id/component_dailytimer_notstarted"
            layout="@layout/component_dailytimer_notstarted"/>
    
    <include
            android:id="@+id/component_dailytimer_started"
            layout="@layout/component_dailytimer_started"/>
    
    <include
            android:id="@+id/component_dailytimer_finished"
            layout="@layout/component_dailytimer_finished"/>
    
</FrameLayout>